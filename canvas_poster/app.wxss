/**app.wxss**/
page{
    position: relative;
    padding: 0;
    margin: 0;
    width: 100%;
    height: 100%;
    z-index: 1;
}
button{
    border: none;
    padding: 0rpx;
    margin: 0rpx;
    background: none;
    line-height: 1.1;
}
button::after{
    border: none;
    -webkit-transform: none;
}
.button-hover{
    color: none;
    background: none;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.header > image{
    background-color: #e2e2e2;
}
